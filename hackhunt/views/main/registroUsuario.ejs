<!DOCTYPE html>
<html lang="es">

<head>
    <%- include('../partials/head'); -%>
    <link rel="stylesheet" href="/css/main/registroUsuario.css" />
</head>
<body>
<!-- CÓDIGO -->
<div class="container">
        <div class="row">
            <div class="col-12 registro wrap">
                <div class="formulario wrap">
                    <form action="/registro" method="post">
                    <label for="user_name"><p>Nombre</p></label>
                    <input type="text" name="user_name" id="user_name" required>
                    <label for="user_lastname"><p>Apellido</p></label>
                    <input type="text" name="user_lastname" id="user_lastname" required>
                    <label for="user_email"><p>Email</p></label>
                    <input type="email" name="user_email" id="user_email" required>
                    <label for="user_passwd"><p>Clave</p></label>
                    <input type="password" name="user_passwd" id="user_passwd" required>
                    <p>o registrate usando</p>
                    <a id="facebook" href="">Facebook</a>
                    <a id="github" href="">Git Hub</a>
                    <a id="google" href="">Google</a>
                    <button type="submit">Comenzar</button>
                    <button type="reset">Limpiar</button>
                </form>
                </div>
            </div>
        </div>
    </div>
<!-- CÓDIGO -->
</body>

</html>